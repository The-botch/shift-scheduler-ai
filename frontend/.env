# Backend API URL
VITE_API_URL=https://shift-scheduler-ai-production.up.railway.app
VITE_BACKEND_API_URL=https://shift-scheduler-ai-production.up.railway.app

# Frontend URL (used for QR codes, etc.)
VITE_FRONTEND_URL=https://shift-scheduler-ai.vercel.app

# LINE LIFF ID
VITE_LIFF_ID=your_liff_id_here

# OpenAI API Key
VITE_OPENAI_API_KEY=your_openai_api_key_here

# OpenAI API Configuration
VITE_OPENAI_MODEL=gpt-4
VITE_OPENAI_MAX_TOKENS=2000

# GitHub Personal Access Token
GH_TOKEN=your_github_token_here

# デフォルト設定値
# ハードコーディングを避けるため、設定値を環境変数で管理

# デフォルトテナントID（マルチテナント対応）
VITE_DEFAULT_TENANT_ID=1

# デフォルト店舗ID
VITE_DEFAULT_STORE_ID=1

# デモ・開発用設定
VITE_DEMO_PLAN_ID=4
VITE_DEMO_STAFF_ID=5
VITE_DEMO_YEAR=2024
VITE_DEMO_MONTH=10

# Password Protection (Simple client-side protection)
# Note: This is NOT secure. The credentials are visible in the built JavaScript files.
# This provides only light protection against casual users, not against determined attackers.
# Set to 'true' to enable password protection (recommended for staging/production only)
VITE_PASSWORD_PROTECTION_ENABLED=true
# Credentials format: "username1:password1,username2:password2"
# Multiple username/password pairs can be specified, separated by commas
VITE_PASSWORD_PROTECTION_CREDENTIALS=admin:password,user:password
# Session duration in milliseconds (optional, default: 3600000 = 1 hour)
# Examples: 1800000 (30 min), 7200000 (2 hours), 86400000 (24 hours)
VITE_PASSWORD_PROTECTION_SESSION_DURATION=10000
